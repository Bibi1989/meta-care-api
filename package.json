{
  "name": "pern",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch-ts": "tsc -w",
    "watch-node": "nodemon ./bin/www",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "drop": "sequelize db:migrate:undo:all",
    "up": "sequelize db:migrate",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && yarn migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=20000",
    "pretest": "cross-env NODE_ENV=test yarn migrate:reset",
    "db:create-test": "cross-env NODE_ENV=test npx sequelize-cli db:create"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/express-fileupload": "^1.1.3",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^26.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node-fetch": "^2.0.3",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.6.3",
    "concurrently": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jest": "^26.0.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.0",
    "node-fetch": "^2.0.0",
    "nodemon": "^2.0.2",
    "sequelize": "^5.21.9",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "^26.4.2"
  },
  "devDependencies": {
    "typescript": "^3.8.5"
  }
}
